<?xml version="1.0" ?>
<sdf version="1.7">
  <world name="empty_with_state">
    <!-- Optional: basic models -->
    <include><uri>model://ground_plane</uri></include>
    <include><uri>model://sun</uri></include>

    <!-- Load the ROS state WORLD plugin (gives /gazebo/set_entity_state etc.) -->
    <plugin name="gazebo_ros_state" filename="libgazebo_ros_state.so">
      <ros>
        <namespace>/</namespace>
      </ros>
    </plugin>
  </world>
</sdf>