        <geometry>
          <box>
            <size>0.8 0.5 0.5</size>
          </box>
        </geometry>
        <material>
          <script>
            <uri>model://goal_marker/materials/scripts/chassis_pattern.material</uri>
            <name>goal_marker/ChassisPattern</name>
          </script>
        </material>
