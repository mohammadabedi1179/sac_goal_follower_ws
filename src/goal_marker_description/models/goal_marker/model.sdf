          </script>
        </material>
      </visual>
    </link>
    <static>0</static>
    <plugin name='goal_diff_drive' filename='libgazebo_ros_diff_drive.so'>
      <ros>
        <namespace>/goal_marker</namespace>
        <remapping>cmd_vel:=/goal_marker/cmd_vel</remapping>
        <remapping>odom:=/goal_marker/odom</remapping>
      </ros>
      <left_joint>left_wheel_joint</left_joint>
      <right_joint>right_wheel_joint</right_joint>
      <wheel_separation>0.5</wheel_separation> <!-- Adjusted to match outer wheel placement (0.5m width) -->
      <wheel_diameter>0.2</wheel_diameter> <!-- Unchanged for radius 0.1m -->
      <max_wheel_torque>20</max_wheel_torque>
      <max_wheel_acceleration>5.0</max_wheel_acceleration>
      <publish_odom>1</publish_odom>
      <publish_wheel_tf>0</publish_wheel_tf>
      <publish_odom_tf>0</publish_odom_tf>
      <odometry_frame>goal_odom</odometry_frame>
      <robot_base_frame>base_link</robot_base_frame>
    </plugin>
  </model>
</sdf>