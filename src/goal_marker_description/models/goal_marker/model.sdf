    </link>
    <joint name='right_wheel_joint' type='revolute'>
      <pose relative_to='base_link'>0 -0.25 0 1.5708 -0 0</pose> <!-- Adjusted y from -0.225 to -0.25 to stick to the outer side (width 0.5m, outer edge at -0.25m) -->
      <parent>base_link</parent>
      <child>right_wheel</child>
      <axis>
        <xyz>0 0 -1</xyz>
        <limit>
          <lower>-1e+16</lower>
          <upper>1e+16</upper>
