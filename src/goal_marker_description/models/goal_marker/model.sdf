<sdf version='1.7'>
  <model name='goal_marker'>
    <link name='base_link'>
      <inertial>
        <pose>0.4 0 0.25 0 -0 0</pose> <!-- Kept the same as previous -->
        <mass>0.6</mass>
        <inertia>
          <ixx>0.0508333</ixx> <!-- Kept the same -->
