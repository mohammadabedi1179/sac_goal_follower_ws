<?xml version="1.0"?>
<robot name="my_robot" xmlns:xacro="https://www.ros.org/wiki/xacro">
  <xacro:include filename="inertial_macros.xacro" />
  
  <!-- BASE LINK -->

  <link name="base_link">
  </link>

    <!-- CHASSIS LINK -->
    
  <joint name="chassis_joint" type="fixed">
      <parent link="base_link"/>
      <child link="chassis"/>
      <origin xyz="0 0 0"/>
  </joint>

  <link name="chassis">
    <inertial>
      <origin xyz="0.3539 -0.33747 0.041313" rpy="0 0 0"/>
      <mass value="25.4291"/>
      <inertia ixx="0.015507" ixy="3.7419E-07" ixz="2.5675E-07" iyy="0.032103" iyz="8.6348E-06" izz="0.043517"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/base_link.STL"/>
      </geometry>
      <material name="">
        <color rgba="0.75294 0.75294 0.75294 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/base_link.STL"/>
      </geometry>
    </collision>
  </link>

  <gazebo reference="chassis">
    <material> Gazebo/DarkGrey </material>
  </gazebo>

  <!--BACK LEFT WHEEL LINK -->

  <joint name="BL_joint" type="continuous">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="BL_link"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="BL_link">
    <inertial>
      <origin xyz="4.8746E-06 -0.03315 4.7136E-11" rpy="0 0 0"/>
      <mass value="1.4314"/>
      <inertia ixx="0.0033197" ixy="3.9016E-07" ixz="-2.4986E-10" iyy="0.0059556" iyz="1.6564E-10" izz="0.0033197"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/BL_link.STL"/>
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1" />
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/BL_link.STL"/>
      </geometry>
    </collision>
  </link>

  <gazebo reference="BL_link">
    <material> Gazebo/Grey </material>
    <mu1>1.0</mu1><mu2>1.0</mu2>
    <!--<kp>1e6</kp><kd>1</kd>-->
  </gazebo>
  <!--FRONT RIGHT WHEEL LINK -->
  
  <joint name="FR_joint" type="continuous">
    <origin xyz="0.659 -0.66108 0" rpy="0 0 0"/>
    <parent link="base_link" />
    <child link="FR_link"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="FR_link">
    <inertial>
      <origin xyz="-4.8761E-06 0.03315 -5.4365E-09" rpy="0 0 0"/>
      <mass value="1.4314"/>
      <inertia ixx="0.0033197" ixy="3.9016E-07" ixz="2.4986E-10" iyy="0.0059556" iyz="-1.6564E-10" izz="0.0033197"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/FR_link.STL"/>
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/FR_link.STL" />
      </geometry>
    </collision>
  </link>

  <gazebo reference="FR_link">
    <material> Gazebo/Grey </material>
    <mu1>1.0</mu1><mu2>1.0</mu2>
    <!--<kp>1e6</kp><kd>1</kd>-->
  </gazebo>

  <!-- FRONT LEFT WHEEL LINK-->

  <joint name="FL_joint" type="continuous">
    <origin xyz="0.659 0 0" rpy="0 0 0"/>
    <parent link="base_link" />
    <child link="FL_link" />
    <axis xyz="0 1 0"/>
  </joint>

  <link name="FL_link">
    <inertial>
      <origin xyz="-4.8748E-06 -0.03315 5.0964E-11" rpy="0 0 0"/>
      <mass value="1.4314"/>
      <inertia ixx="0.0033197" ixy="-3.9016E-07" ixz="-2.729E-10" iyy="0.0059556" iyz="1.6538E-10" izz="0.0033197"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh
          filename="file://$(find my_robot_description)/models/my_robot_description/meshes/FL_link.STL"/>
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/FL_link.STL" />
      </geometry>
    </collision>
  </link>

  <gazebo reference="FL_link">
  <material> Gazebo/Grey </material>
  <mu1>1.0</mu1><mu2>1.0</mu2>
  <!--<kp>1e6</kp><kd>1</kd>-->
  </gazebo>
  
  <!-- BACK RIGHT WHEEL LINK-->
  
  <joint name="BR_joint" type="continuous">
    <origin xyz="0 -0.66108 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="BR_link"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="BR_link">
    <inertial>
      <origin xyz="4.8766E-06 0.03315 -5.3194E-09" rpy="0 0 0"/>
      <mass value="1.4314" />
      <inertia ixx="0.0033197" ixy="-3.9016E-07" ixz="2.729E-10" iyy="0.0059556" iyz="-1.6538E-10" izz="0.0033197"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/BR_link.STL" />
      </geometry>
      <material name="">
        <color rgba="0.79216 0.81961 0.93333 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="file://$(find my_robot_description)/models/my_robot_description/meshes/BR_link.STL" />
      </geometry>
    </collision>
  </link>

  <gazebo reference="BR_link">
    <material> Gazebo/Grey </material>
    <mu1>1.0</mu1><mu2>1.0</mu2>
    <!--<kp>1e6</kp><kd>1</kd>-->
  </gazebo>

  <!-- HANDLE MIDDLE LINK-->
    
  <joint name="handle_middle_joint" type="fixed">
    <parent link="base_link" />
    <child link="handle_middle" />
    <origin xyz="0.665 -0.33059 0.43" rpy="0 0 0" />
    <axis xyz="0 0 0" />
  </joint>

  <link name="handle_middle">
      <visual>
          <origin xyz="0 0 0"/>
          <geometry>
            <cylinder length="0.5" radius="0.015"/>
          </geometry>
      </visual>
      <collision>
          <origin xyz="0 0 0" />
          <geometry>
            <cylinder length="0.5" radius="0.015"/>
          </geometry>
      </collision>
      <xacro:inertial_cylinder mass="0.1" length="0.5" radius="0.015">
        <origin xyz="0 0 0" rpy="0 0 0"/>
      </xacro:inertial_cylinder>
  </link>
  <gazebo reference="handle_middle">
      <material> Gazebo/DarkGrey </material>
  </gazebo>

  <!-- HANDLE LINK-->
  
  <joint name="handle_joint" type="fixed">
      <parent link="handle_middle"/>
      <child link="handle"/>
      <origin xyz="0 0 0.25" rpy="${pi/2} 0 0"/>
      <axis xyz="0 0 0"/>
  </joint>

  <link name="handle">
      <visual>
          <origin xyz="0 0 0"/>
          <geometry>
            <cylinder length="0.4" radius="0.015"/>
          </geometry>
      </visual>
      <collision>
          <origin xyz="0 0 0" />
          <geometry>
              <cylinder length="0.4" radius="0.015"/>
          </geometry>
      </collision>
      <xacro:inertial_cylinder mass="0.1" length="0.4" radius="0.015">
          <origin xyz="0 0 0" rpy="0 0 0"/>
      </xacro:inertial_cylinder>
  </link>
  <gazebo reference="handle">
      <material> Gazebo/DarkGrey </material>
  </gazebo>

  <!-- Transmissions for wheel joints -->
    <transmission name="left_front_wheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="FL_joint">
        <hardwareInterface>velocity</hardwareInterface>
    </joint>
    <actuator name="left_front_wheel_motor">
        <hardwareInterface>velocity</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    </transmission>

    <transmission name="left_back_wheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="BL_joint">
        <hardwareInterface>velocity</hardwareInterface>
    </joint>
    <actuator name="left_back_wheel_motor">
        <hardwareInterface>velocity</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    </transmission>

    <transmission name="right_front_wheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="FR_joint">
        <hardwareInterface>velocity</hardwareInterface>
    </joint>
    <actuator name="right_front_wheel_motor">
        <hardwareInterface>velocity</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    </transmission>

    <transmission name="right_back_wheel_trans">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="BR_joint">
        <hardwareInterface>velocity</hardwareInterface>
    </joint>
    <actuator name="right_back_wheel_motor">
        <hardwareInterface>velocity</hardwareInterface>
        <mechanicalReduction>1</mechanicalReduction>
    </actuator>
    </transmission>

</robot>